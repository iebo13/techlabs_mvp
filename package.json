{
  "name": "techlabs_mvp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "// Development": "Development and local server scripts",
    "dev": "vite",
    "dev:host": "vite --host 0.0.0.0 --port 3000",
    "dev:debug": "vite --force --debug",
    "dev:no-hmr": "vite --hmr false",
    "// Deployment": "Deployment scripts",
    "deploy": "npm run build && firebase deploy",
    "// Build": "Build and preview scripts",
    "build": "tsc -b && vite build",
    "build:analyze": "npm run build && npm run analyze",
    "build:clean": "rimraf dist && npm run build",
    "preview": "vite preview",
    "analyze": "npx vite-bundle-analyzer dist/stats.html",
    "// Testing": "Test execution scripts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:run": "jest --passWithNoTests",
    "test:update": "jest --updateSnapshot",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:silent": "jest --silent",
    "// Code Quality": "Linting, formatting, and type checking",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:staged": "lint-staged",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "// Performance": "Performance monitoring and analysis",
    "lighthouse": "lighthouse http://localhost:4173 --output=html --output-path=./lighthouse-report.html --chrome-flags='--headless'",
    "lighthouse:ci": "lighthouse http://localhost:4173 --output=json --output-path=./lighthouse-report.json --chrome-flags='--headless'",
    "performance": "npm run build && npm run preview & sleep 10 && npm run lighthouse:ci && pkill -f 'vite preview'",
    "// Utilities": "Utility and maintenance scripts",
    "clean": "rm -rf dist coverage .nyc_output .firebase node_modules && rm package-lock.json",
    "unimported": "npx unimported",
    "convert-imports": "node scripts/convert-imports.js",
    "convert-imports:dry-run": "node scripts/convert-imports.js --dry-run",
    "// Quality Gates": "Comprehensive quality checks",
    "check": "tsc -b && npm run lint && npm run typecheck && npm run test:run && npm run format",
    "check:fix": "npm run format:fix && npm run lint:fix && npm run typecheck",
    "check:all": "npm run check && npm run test:coverage",
    "precommit": "npm run lint:staged && npm run typecheck && npm run test:run",
    "// Setup": "Project setup and initialization",
    "setup": "node scripts/setup-project.js",
    "validate": "node scripts/validate-setup.js",
    "prepare": "husky",
    "postinstall": "npm run prepare"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/inter": "^5.2.6",
    "@fontsource/poppins": "^5.2.6",
    "@hookform/resolvers": "^5.2.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@tanstack/react-query": "^5.85.5",
    "date-fns": "^4.1.0",
    "firebase": "^12.1.0",
    "i18next": "^25.5.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.62.0",
    "react-i18next": "^15.7.3",
    "react-router-dom": "^7.8.1",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.33.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@types/react-helmet-async": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@vitejs/plugin-react": "^5.0.0",
    "chrome-launcher": "^1.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-unicorn": "^60.0.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-transform-stub": "^2.0.0",
    "lighthouse": "^12.8.2",
    "lint-staged": "^15.2.2",
    "msw": "^2.11.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.5",
    "rollup-plugin-visualizer": "^6.0.3",
    "sharp": "^0.34.3",
    "ts-jest": "^29.4.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "web-vitals": "^3.5.2"
  }
}
